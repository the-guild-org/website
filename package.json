    "@guild-docs/client": "2.1.1",
    "@mapbox/rehype-prism": "0.8.0",
    "@mdx-js/loader": "2.1.1",
    "@mdx-js/react": "2.1.1",
    "@next/mdx": "12.1.6",
    "@radix-ui/react-tooltip": "0.1.7",
    "@slack/web-api": "6.7.1",
    "@stitches/react": "1.2.8",
    "@tailwindcss/line-clamp": "0.4.0",
    "@theguild/components": "1.11.8",
    "algoliasearch": "4.13.1",
    "autoprefixer": "10.4.7",
    "axios": "0.27.2",
    "clsx": "1.1.1",
    "cors": "2.8.5",
    "date-fns": "2.28.0",
    "express": "4.18.1",
